<script src="httpss://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="httpss://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
<script src="httpss://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
<script>

$(document).ready(function(){


    $("#schoolName").text(window.localStorage.getItem("schoolName").toLowerCase());
    $("#logo").attr("src", window.localStorage.getItem("logo"));

  });

</script>
</head>
<body style="height: 100%">
    <div
      id="root"
      style="height: 100%; margin: 0px; padding: 0px"
      class="container-fluid"
    >
    <div style="height:100%;background-color:rgb(223,224,228);margin:0px;padding:0px;width:100%">
    <div
    class='row violet-template'
    id='schoolHeader'
    style="
      width: 100%;
      margin: 0px;"
    >
    <div class='col-sm-12' style="padding:20px">
      <img
        id='logo'
        src= ""
        style="margin: 0 auto; display:block;height:90px"
      />
      <br></br>
      <h1
        id='schoolName'
        style="text-align:center;color: white;font-family: Cinzel"
      >
      
      </h1>
    </div>
 
  </div>
  <div
  class='row'
  style="padding-top:15px;margin-bottom:15px;width:100%;margin-left:0px;margin-right:0px"
>
  <div class='col-xl-10 offset-xl-1' id='header'>
    <button
      class='btn btn-primary btn-lg violet-template'
      style="border-radius:50%;float:right;"
    >
      <i
        class='fa fa-power-off'

      ></i>
    </button>

    <span
      style="position:absolute;right:20px;top:70px;font-size:12px"
    >
      <a class='logout'>
        <b style="color:black;">LOGOUT</b>
      </a>
    </span>

    <img
      id='studid'
      src="./public/dummyPic.png"
      style="margin-left:15px;width:150px;height:160px;border:2px solid rgb(229, 230, 233);border-bottom:6px solid rgb(229, 230, 233)"
    />

    <div id='userBasicDetails'>
      <h2 style="padding:0px;margin:0px">
        Welcome to Student page!
      </h2>

      <hr
        style="height:1px;background-color:black;color:black;margin-top:5px;margin-bottom:5px"
      />
      <div id='detailsWhiteBox'>
        <div id='basicDataCol1' class='basicDataCols'>
          <label class='dataLabels'>
            FirstName: 
          </label>
          <br />
          <label class='dataLabels'>MiddleName:</label>
          <br />
          <label class='dataLabels'>
            LastName: 
          </label>
        </div>
        <div id='basicDataCol2' class='basicDataCols'>
          <label class='dataLabels'>
            RollNo:
          </label>
          <br />
          <label class='dataLabels'>
            Standard:
          </label>
          <br />
          <label class='dataLabels'>
            DateOfBirth:
          </label>
        </div>
        <div
          id='basicDataCol3'
          class='basicDataCols'
          style="vertical-align:top"
        >
          <label class='dataLabels'>
            BloodGroup:
          </label>
          <br />
          <label class='dataLabels'>
            Gender:
          </label>
        </div>
      </div>
    </div>
  </div>
</div>
<div  class='row' style="background-color:rgb(223, 224, 228);width:100%;margin-left:0px;margin-right:0px">
<div class='offset-xl-1 col-xl-10' id='DashBoardContainer'>
  <div class='row' style="margin-left:0px;margin-right:0px">
    <div class='col-lg-3 col-md-3 col-sm-3' id='buttonMenu'>
    <div class='buttonContainer'>
        <div
          class='menuIconDiv violet-template'
          style="padding:0px;text-align:center;"
        >
          <i class='fa fa-bars' aria-hidden='true'></i>
        </div>
      </div>
      <div class='buttonContainer'>
      
        <button
          class='btn btn-primary btn-block attendanceStatus violet-template'
          name='Attendance'
        >
          Attendance Status
        </button>
      </div>
 
      <div class='buttonContainer'>
        <button
          class='btn btn-primary btn-block assignmentDetails violet-template'
        
          
          name='Assignments'
        >
          Assignment Details
        </button>
      </div>
    
    
      <div class='buttonContainer'>
        <button
          class='btn btn-primary btn-block parentDetails violet-template'
       
          
          name='Library'
        >
          Virtual Library
        </button>
      </div>
      
     
      <div class='buttonContainer'>
        <button
          class='btn btn-primary btn-block feesDetails violet-template'
        
         
          name='Lecture'
        >
          Online Lecture
        </button>
      </div>
    
    
      <div class='buttonContainer'>
        <button
          class='btn btn-primary btn-block resultDetails violet-template'
        
        >
          Result Details
        </button>
      </div>
   
   
      <div class='buttonContainer'>
        <button
          class='btn btn-primary btn-block schoolEvents violet-template'
         
        >
          School Events
        </button>
      </div>
     
  
      <div class='buttonContainer'>
        <button
          class='btn btn-primary btn-block transportDetails violet-template'
        
        >
          Transport Details
        </button>
      </div>
     
    
      <div class='buttonContainer'>
        <button
          class='btn btn-primary btn-block libraryBooks violet-template'
          
        >
          Library Books
        </button>
      </div>
   
     
      <div class='buttonContainer'>
        <button
          class='btn btn-primary btn-block addressDetails violet-template'
          
        >
          Address Details
        </button>
      </div>
     
    
      <div class='buttonContainer'>
        <button
          class='btn btn-primary btn-block contactDetails violet-template'
        
        >
          Contact Details
        </button>
      </div>
    </div>
    <div
      class="col-lg-9 col-md-9 col-sm-9"
      id='queryResultContainer'
    >
    <div >
      <h5 style="margin:0 auto;width:30%;text-align:center">
        <b>Attendance Status</b>
      </h5>
      <hr
        style="height:1px;background-color:black;color:black;margin-top:5px;margin-bottom:5px"
      />
      <div class='row'>
        <div class='col-12'>
          <div class='form-check'>
            <input
              type='radio'
              class='form-check-input'
              name='type'
              value='monthly'
            />
            <label>
              1.) For month wise attendance status, please select a month and
              year:
            </label>
            <br />
          </div>
          <select
            class='form-control'
            id='months'
            name='month'
            style="width:170px;display:inline-block"
          >
            <option value='0'>SELECT MONTH</option>
            <option value='1'>January</option>
            <option value='2'>February</option>
            <option value='3'>March</option>
            <option value='4'>April</option>
            <option value='5'>May</option>
            <option value='6'>June</option>
            <option value='7'>July</option>
            <option value='8'>August</option>
            <option value='9'>September</option>
            <option value='10'>October</option>
            <option value='11'>November</option>
            <option value='12'>December</option>
          </select>
          <select
            class='form-control'
            id='year'
            name='year'
            style="width:170px;display:inline-block"
          >
            <option value='0'>SELECT YEAR</option>
            <option value='2020'>2020</option>
            <option value='2021'>2021</option>
            <option value='2022'>2022</option>
          </select>

          <br />
          <br/>
        </div>
        <div class='col-12'>
          <div class='form-check'>
            <input
              type='radio'
              class='form-check-input'
              name='type'
              value='specific'
              required
            />
            <label>
              2.) For attendance status of a specific period, please enter start
              date and end date:
            </label>
          </div>
         
          <div style="display:inline-block;width:170px">
            <input
              placeholder='From Date'
              class='form-control'
              style="width:170px;display:inline-block"
            />
          </div>
          <div style="display:inline-block;width:170px">
            <input
              placeholder='Till Date'
              class='form-control'
              style="width:170px;display:inline-block"
            />
          </div>
          <br />
          <br/>
        </div>
        
        <div class='col-12'>
          <div class='form-check'>
            <input
              type='radio'
              class='form-check-input'
              name='type'
            />
            <label>3.) For complete attendance status till date.</label>
          </div>
          <br />
          <div class='col-12' style="text-align:center">
            <button
              type='submit'
              class='btn btn-primary btn-sm violet-template'
              id='tillDateAttendance'
            >
              Check Attendance
            </button>
          </div>
        </div>
      </div>

    


    </div>
    </div>
  </div>
</div>
</div>
</div>
</div>
